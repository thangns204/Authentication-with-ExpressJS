	<!-- Start main body Area -->
	<div class="limiter">
		<div class="container-table100">
			<div class="wrap-table100">
				<div class="table100 ver1 m-b-110">
					<div class="table100-head">
						<table>
							<thead>
								<tr class="row100 head">
									<th class="cell100 column1">No</th>
									<th class="cell100 column2">Type</th>
									<th class="cell100 column3">Duration</th>
									<th class="cell100 column4">Value</th>
								</tr>
							</thead>
						</table>
					</div>

					<div class="table100-body js-pscroll">
						<table>
							<tbody class="row100 body">
								<% for (var k = 0;  k < ticket.length; k++){ %>
									<% for (var i = 0;  i < ticket[k].properties.duration.length; i++) { %>	
		  							<tr>
		  								<td class="cell100 column1"><%= i + k + 1 %></td>
		  								<td class="cell100 column2"><%= ticket[k].type %></td>
		  								<td class="cell100 column3"><%= ticket[k].properties.duration[i] %></td>
		  								<td class="cell100 column4"><%= ticket[k].properties.value[i] %></td>
		  							</tr>
		  							<% } %>
	  							<% } %>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>

	
